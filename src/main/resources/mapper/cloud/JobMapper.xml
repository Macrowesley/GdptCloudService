<mapper namespace="com.gdpt.cloudService.mapper.JobMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cc.mrbird.febs.cloud.entity.Job">
        <id column="job_id" property="jobId" />
        <result column="name" property="name" />
        <result column="description" property="description" />
